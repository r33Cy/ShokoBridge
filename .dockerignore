# Git and VCS - prevents sending entire git history to the build context
.git
.github
.gitignore

# Python virtual environment
.venv

# Python cache
__pycache__/
*.pyc
*.pyo
*.pyd

# IDE and editor directories
.idea
.vscode

# Testing artifacts - not needed in the final image
testing/

